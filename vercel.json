{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite" , 
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/dist/index.html"
    }
  ]
}

